<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
	<extension point="org.emftext.access.syntax">
		<metaInformationProvider class="org.emftext.language.AdaptiveCyberDefense.resource.AdaptiveCyberDefense.mopp.AdaptiveCyberDefenseMetaInformation" id="org.emftext.language.AdaptiveCyberDefense.resource.AdaptiveCyberDefense.mopp.AdaptiveCyberDefenseMetaInformation">
		</metaInformationProvider>
	</extension>
	
	<extension id="org.emftext.language.AdaptiveCyberDefense.resource.AdaptiveCyberDefense.problem" name="EMFText Problem" point="org.eclipse.core.resources.markers">
		<persistent value="true">
		</persistent>
		<super type="org.eclipse.core.resources.problemmarker">
		</super>
		<super type="org.eclipse.core.resources.textmarker">
		</super>
		<super type="org.eclipse.emf.ecore.diagnostic">
		</super>
	</extension>
	
	<extension id="org.emftext.language.AdaptiveCyberDefense.resource.AdaptiveCyberDefense.problem.syntax_error" name="EMFText Problem" point="org.eclipse.core.resources.markers">
		<persistent value="true">
		</persistent>
		<super type="org.emftext.language.AdaptiveCyberDefense.resource.AdaptiveCyberDefense.problem">
		</super>
	</extension>
	
	<extension id="org.emftext.language.AdaptiveCyberDefense.resource.AdaptiveCyberDefense.problem.print_problem" name="EMFText Problem" point="org.eclipse.core.resources.markers">
		<persistent value="true">
		</persistent>
		<super type="org.emftext.language.AdaptiveCyberDefense.resource.AdaptiveCyberDefense.problem">
		</super>
	</extension>
	
	<extension id="org.emftext.language.AdaptiveCyberDefense.resource.AdaptiveCyberDefense.problem.unresolved_reference" name="EMFText Problem" point="org.eclipse.core.resources.markers">
		<persistent value="true">
		</persistent>
		<super type="org.emftext.language.AdaptiveCyberDefense.resource.AdaptiveCyberDefense.problem">
		</super>
	</extension>
	
	<extension id="org.emftext.language.AdaptiveCyberDefense.resource.AdaptiveCyberDefense.problem.analysis_problem" name="EMFText Problem" point="org.eclipse.core.resources.markers">
		<persistent value="true">
		</persistent>
		<super type="org.emftext.language.AdaptiveCyberDefense.resource.AdaptiveCyberDefense.problem">
		</super>
	</extension>
	
	<extension id="org.emftext.language.AdaptiveCyberDefense.resource.AdaptiveCyberDefense.problem.builder_error" name="EMFText Problem" point="org.eclipse.core.resources.markers">
		<persistent value="true">
		</persistent>
		<super type="org.emftext.language.AdaptiveCyberDefense.resource.AdaptiveCyberDefense.problem">
		</super>
	</extension>
	
	<extension id="org.emftext.language.AdaptiveCyberDefense.resource.AdaptiveCyberDefense.nature" name="AdaptiveCyberDefense nature" point="org.eclipse.core.resources.natures">
		<runtime>
			<run class="org.emftext.language.AdaptiveCyberDefense.resource.AdaptiveCyberDefense.mopp.AdaptiveCyberDefenseNature">
			</run>
		</runtime>
		<builder id="org.emftext.language.AdaptiveCyberDefense.resource.AdaptiveCyberDefense.builder">
		</builder>
	</extension>
	
	<extension point="org.eclipse.core.resources.builders" id="org.emftext.language.AdaptiveCyberDefense.resource.AdaptiveCyberDefense.builder" name="AdaptiveCyberDefense Builder">
		<builder hasNature="true">
			<run class="org.emftext.language.AdaptiveCyberDefense.resource.AdaptiveCyberDefense.mopp.AdaptiveCyberDefenseBuilderAdapter">
			</run>
		</builder>
	</extension>
	
	<extension-point id="org.emftext.language.AdaptiveCyberDefense.resource.AdaptiveCyberDefense.default_load_options" name="Default Load Options" schema="schema/default_load_options.exsd">
	</extension-point>
	
	<extension point="org.eclipse.emf.ecore.extension_parser">
		<parser class="org.emftext.language.AdaptiveCyberDefense.resource.AdaptiveCyberDefense.mopp.AdaptiveCyberDefenseResourceFactoryDelegator" type="AdaptiveCyberDefense">
		</parser>
	</extension>
	
	<extension-point id="org.emftext.language.AdaptiveCyberDefense.resource.AdaptiveCyberDefense.additional_extension_parser" name="Additional Extension Parser" schema="schema/additional_extension_parser.exsd">
	</extension-point>
	
	<extension point="org.eclipse.debug.core.breakpoints">
		<breakpoint class="org.emftext.language.AdaptiveCyberDefense.resource.AdaptiveCyberDefense.debug.AdaptiveCyberDefenseLineBreakpoint" id="org.emftext.language.AdaptiveCyberDefense.resource.AdaptiveCyberDefense.debug.breakpoint" markerType="org.emftext.language.AdaptiveCyberDefense.resource.AdaptiveCyberDefense.debug.lineBreakpoint.marker" name="AdaptiveCyberDefense Breakpoint">
		</breakpoint>
	</extension>
	
	<extension id="org.emftext.language.AdaptiveCyberDefense.resource.AdaptiveCyberDefense.debug.lineBreakpoint.marker" point="org.eclipse.core.resources.markers">
		<super type="org.eclipse.debug.core.lineBreakpointMarker">
		</super>
		<super type="org.eclipse.core.resources.textmarker">
		</super>
		<persistent value="true">
		</persistent>
	</extension>
	
	<extension point="org.eclipse.debug.core.launchConfigurationTypes">
		<launchConfigurationType id="org.emftext.language.AdaptiveCyberDefense.resource.AdaptiveCyberDefense.ui.launchConfigurationType" delegate="org.emftext.language.AdaptiveCyberDefense.resource.AdaptiveCyberDefense.launch.AdaptiveCyberDefenseLaunchConfigurationDelegate" modes="run,debug" name="AdaptiveCyberDefense Application" sourceLocatorId="org.emftext.language.AdaptiveCyberDefense.resource.AdaptiveCyberDefense.ui.sourceLocator" sourcePathComputerId="org.emftext.language.AdaptiveCyberDefense.resource.AdaptiveCyberDefense.ui.sourcePathComputer" delegateName="AdaptiveCyberDefense Launch Tooling" delegateDescription="This will run or debug .AdaptiveCyberDefense files.">
		</launchConfigurationType>
	</extension>
	
	<extension point="org.eclipse.debug.core.sourcePathComputers">
		<sourcePathComputer class="org.emftext.language.AdaptiveCyberDefense.resource.AdaptiveCyberDefense.debug.AdaptiveCyberDefenseSourcePathComputerDelegate" id="org.emftext.language.AdaptiveCyberDefense.resource.AdaptiveCyberDefense.ui.sourcePathComputer">
		</sourcePathComputer>
	</extension>
	
	<extension point="org.eclipse.debug.core.sourceLocators">
		<sourceLocator id="org.emftext.language.AdaptiveCyberDefense.resource.AdaptiveCyberDefense.ui.sourceLocator" class="org.emftext.language.AdaptiveCyberDefense.resource.AdaptiveCyberDefense.debug.AdaptiveCyberDefenseSourceLocator" name="AdaptiveCyberDefense Source Locator">
		</sourceLocator>
	</extension>
	
</plugin>
