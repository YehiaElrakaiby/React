	<extension point="org.eclipse.debug.ui.launchShortcuts">
		<shortcut label="AdaptiveCyberDefense Application" icon="/icons/launch_shortcut_icon.gif" helpContextId="org.emftext.language.AdaptiveCyberDefense.launch" modes="run,debug" class="org.emftext.language.AdaptiveCyberDefense.resource.AdaptiveCyberDefense.ui.launch.AdaptiveCyberDefenseLaunchShortcut" description="Launch a AdaptiveCyberDefense model" id="org.emftext.language.AdaptiveCyberDefense.launchShortcut">
			<description description="Run AdaptiveCyberDefense model" mode="run">
			</description>
			<description description="Debug AdaptiveCyberDefense model" mode="debug">
			</description>
			<contextualLaunch>
				<enablement>
					<with variable="selection">
						<count value="1">
						</count>
						<iterate>
							<test property="org.emftext.language.AdaptiveCyberDefense.isLaunchable">
							</test>
						</iterate>
					</with>
				</enablement>
			</contextualLaunch>
			<configurationType id="org.emftext.language.AdaptiveCyberDefense.resource.AdaptiveCyberDefense.ui.launchConfigurationType">
			</configurationType>
		</shortcut>
	</extension>
	
